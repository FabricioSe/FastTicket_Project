@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}
<body style="background-color: #2E2E2E">
    <div class="card container-fluid box" style="width: 700px; border-radius: 20px;">
        <div class="card-body">
            <h1 class="text-danger text-center">@ViewData["Title"]</h1>
            <hr/>
            <h3 class="text-danger text-center">This account has been locked out, please try again later.</h3>
        </div>
    </div>
</body>
